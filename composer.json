{
    "name": "vlastimilvasek/discogs-api",
    "description": "A simple Discogs Api wrapper using Guzzle.",
    "keywords": [
        "Discogs Api"
    ],
    "homepage": "https://github.com/vlastimilvasek/discogs-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolita Grazyte",
            "email": "hello@jolitagraze.com",
            "homepage": "http://jolitagraze.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.4",
        "illuminate/support": "*",
        "guzzlehttp/guzzle":"*",
        "mockery/mockery": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Jolita\\DiscogsApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jolita\\DiscogsApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
